{
  "name": "db",
  "packageManager": "yarn@3.2.0",
  "type": "module",
  "bin": "dist/index.js",
  "scripts": {
    "dl": "zx scripts/csv.mjs",
    "temp": "zx scripts/temp.mjs",
    "dev": "rimraf build && tsc && concurrently \"tsc --watch\" \"nodemon -r dotenv/config build/index.js\"",
    "cleanup": "rimraf .*-audit.json & rimraf *.log & rimraf build && rimraf dist && rimraf bin",
    "build": "yarn cleanup && yarn node -r dotenv/config esbuild.config.cjs",
    "pkg:linux": "yarn build && pkg package.json --options optimize_for_size,max_old_space_size=64,use-idle-notification",
    "make": "zx make.mjs"
  },
  "devDependencies": {
    "@types/big.js": "^6.1.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.22",
    "@types/node-cache": "^4.2.5",
    "concurrently": "^7.0.0",
    "dotenv": "^16.0.0",
    "esbuild": "^0.14.36",
    "nodemon": "^2.0.15",
    "pkg": "^5.6.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3",
    "zx": "^6.0.7"
  },
  "dependencies": {
    "big.js": "^6.1.1",
    "common-util": "../common-util/common-util-9.1.0.tgz",
    "cors": "^2.8.5",
    "date-fns": "^2.28.0",
    "express": "^4.17.3",
    "helmet": "^5.0.2",
    "mariadb": "^3.0.0",
    "node-cache": "^5.1.2",
    "zod": "^3.14.2"
  },
  "pkg": {
    "targets": [
      "node17-linux-x64"
    ],
    "outputPath": "dist/linux"
  }
}
