  "devDependencies": {
    "@types/big.js": "^6.1.3",
    "@types/node": "^17.0.16",
    "dotenv": "^16.0.0",
    "esbuild": "^0.14.21",
    "pkg": "^5.5.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@binance/connector": "^1.7.0",
    "axios": "^0.25.0",
    "big.js": "^6.1.1",
    "poloniex-node-api": "^5.0.0",
    "winston": "^3.5.1",
    "winston-csv-format": "^2.0.2",
    "winston-daily-rotate-file": "^4.6.0"
  }
